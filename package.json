{
  "name": "transit-arrivals-widget",
  "description": "Build a realtime transit arrival tool from GTFS and GTFS-RT.",
  "version": "0.1.4",
  "keywords": [
    "transit",
    "gtfs",
    "transportation",
    "gtfs-rt"
  ],
  "private": false,
  "author": "Brendan Nee <brendan@blinktag.com>",
  "homepage": "https://github.com/BlinkTagInc/transit-arrivals-widget",
  "bugs": {
    "url": "https://github.com/BlinkTagInc/transit-arrivals-widget"
  },
  "repository": "git://github.com/BlinkTagInc/transit-arrivals-widget",
  "contributors": [
    "Wojciech Kulesza <wojciech.kulesza@goeuropa.eu>"
  ],
  "license": "MIT",
  "scripts": {
    "start": "node ./app",
    "test": "xo"
  },
  "bin": {
    "transit-arrivals-widget": "bin/transit-arrivals-widget.js"
  },
  "main": "index",
  "dependencies": {
    "chalk": "^4.1.0",
    "express": "^4.17.1",
    "fs-extra": "^9.1.0",
    "gtfs": "^2.4.3",
    "js-beautify": "^1.13.13",
    "lodash": "^4.17.21",
    "morgan": "^1.10.0",
    "pretty-error": "^3.0.3",
    "pug": "^3.0.2",
    "sanitize-filename": "^1.6.3",
    "timer-machine": "^1.1.0",
    "toposort": "^2.0.2",
    "untildify": "^4.0.0",
    "yargs": "^16.2.0"
  },
  "devDependencies": {
    "xo": "^0.39.1"
  },
  "engines": {
    "node": ">= 14.15.4"
  },
  "xo": {
    "esnext": true,
    "space": true,
    "rules": {
      "camelcase": [
        "error",
        {
          "properties": "never"
        }
      ],
      "object-curly-spacing": [
        "error",
        "always"
      ]
    }
  },
  "false": {}
}
